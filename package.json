{
  "name": "esutils",
  "version": "0.0.1",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/esutils/esutils.git"
  },
  "scripts": {
    "build": "yarn run build-async-semaphore && yarn run build-deferred && yarn run build-delay && yarn run build-invert && yarn run build-dns-packet && yarn run build-invert && yarn run build-mqtt-packet",
    "build-async-semaphore": "cd packages/async-semaphore && yarn build",
    "build-deferred": "cd packages/deferred && yarn build",
    "build-delay": "cd packages/delay && yarn build",
    "build-dns-packet": "cd packages/dns-packet && yarn build",
    "build-invert": "cd packages/invert && yarn build",
    "build-mqtt-packet": "cd packages/mqtt-packet && yarn build"
  },
  "workspaces": [
    "packages/async-semaphore",
    "packages/deferred",
    "packages/delay",
    "packages/dns-packet",
    "packages/empty",
    "packages/invert",
    "packages/mqtt-client",
    "packages/mqtt-packet"
  ],
  "devDependencies": {
    "@types/jest": "^27.5.1",
    "@types/node": "^17.0.35",
    "@typescript-eslint/eslint-plugin": "^5.26.0",
    "@typescript-eslint/parser": "^5.26.0",
    "eslint": "^8.16.0",
    "eslint-config-airbnb": "^19.0.4",
    "eslint-config-airbnb-typescript": "^17.0.0",
    "eslint-plugin-import": "^2.26.0",
    "eslint-plugin-jest": "^26.2.2",
    "eslint-plugin-jsx-a11y": "^6.5.1",
    "eslint-plugin-react": "^7.30.0",
    "eslint-plugin-react-hooks": "^4.5.0",
    "jest": "^28.1.0",
    "ts-loader": "9.3.0",
    "ts-jest": "^28.0.3",
    "ts-node": "^10.8.0",
    "typescript": "^4.6.4",
    "webpack": "^5.72.1",
    "webpack-cli": "^4.9.2",
    "webpack-dev-server": "^4.9.0"
  },
  "private": true
}
